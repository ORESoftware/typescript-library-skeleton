{
  "compileOnSave": false,
  "allowJs": false,
  "compilerOptions": {
    "declaration": false,
    "allowJs": false,
    "pretty": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "allowUnreachableCode": true,
    "lib": [
      "es2015",
      "es2016",
      "es2017"
    ]
  }
}
